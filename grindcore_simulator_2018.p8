t = 0

function object()
	result = {}
	
	
	
	return result
end

function keep_time()
	t += 1;
	if t > 63 then
		t = 0
	end
end

function big_spr(sprite, x, y)
	spr(sprite, x, y)
	spr(sprite + 1, x + 8, y)
	spr(sprite + 16, x, y + 8)
	spr(sprite + 17, x + 8, y + 8)
end
