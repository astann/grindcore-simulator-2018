version 16
__lua__
state = 0 // menu

function _init()

end

function _update()
	keep_time()
	
	if state == 0 then
		update_menu()
	elseif state == 1 then
		update_game()
	end
end

function _draw()
	if state == 0 then
		draw_menu()
	elseif state == 1 then
		draw_game()
	end
end

